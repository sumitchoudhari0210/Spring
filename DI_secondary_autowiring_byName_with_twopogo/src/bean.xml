<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">
        
      
       
        <bean id="addr" class="com.autowrite.byname.Address">
        <property name="areaname" value="karvenagar"></property>
        <property name="cityname" value="pune"></property>
         </bean>
         
         <bean id="empaddr" class="com.autowrite.byname.Address">
        <property name="areaname" value="akurdi"></property>
        <property name="cityname" value="pimpri-chinchwad"></property>
         </bean>
           
        <bean id="s"   class="com.autowrite.byname.Student" autowire="byName">
         <property name="rollno" value="101"></property>
         <property name="name" value="Rahul"></property>
        </bean>
        
        
         <bean id="e"   class="com.autowrite.byname.Employee" autowire="byName">
         <property name="empid" value="102"></property>
         <property name="empname" value="shyam"></property>
        </bean>
      
     
        </beans>